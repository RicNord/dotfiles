#include "%L"

# åÅ
<Multi_key> <parenleft> <parenleft>	: "["	bracketleft # LEFT SQUARE BRACKET
<Multi_key> <parenleft> <minus>		: "{"	braceleft # LEFT CURLY BRACKET

<Multi_key> <bracketleft>		: "å"	aring # LATIN SMALL LETTER A WITH RING ABOVE
<Multi_key> <braceleft>			: "Å"	Aring # LATIN CAPITAL LETTER A WITH RING ABOVE


# äÄ
<dead_acute> <space>			: "'"	apostrophe # APOSTROPHE
<dead_diaeresis> <space>		: "\""	quotedbl # QUOTATION MARK

<Multi_key> <apostrophe>		: "ä"	adiaeresis # LATIN SMALL LETTER A WITH DIAERESIS
<Multi_key> <quotedbl>			: "Ä"	Adiaeresis # LATIN CAPITAL LETTER A WITH DIAERESIS

# öÖ
<Multi_key> <semicolon>			: "ö"	odiaeresis # LATIN SMALL LETTER O WITH DIAERESIS
<Multi_key> <colon>			: "Ö"	Odiaeresis # LATIN CAPITAL LETTER O WITH DIAERESIS

